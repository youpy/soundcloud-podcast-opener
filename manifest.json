{
    "name" : "Soundcloud Podcast Opener",
    "version" : "0.0.5",
    "author" : "youpy",
    "icons" : {
        "16" : "icon.png",
        "48" : "icon.png",
        "128" : "icon.png"
    },
    "description" : "Open podcast for soundcloud.com user tracks.",
    "background" : {
        "scripts" : ["app.js", "background.js"]
    },
    "page_action": {
        "default_icon": "icon.png",
        "default_title": "open podcast",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs"],
    "manifest_version" : 2
}
